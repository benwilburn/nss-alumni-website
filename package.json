{
  "name": "alumni-home",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "babel-eslint": "^9.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "immutable": "^4.0.0-rc.12",
    "jss": "^9.8.7",
    "jss-expand": "^5.3.0",
    "moment": "^2.20.1",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-jss": "^8.6.1",
    "react-markdown": "^4.0.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3"
  },
  "homepage": "https://nss-alumni.github.io/alumni-home",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix .",
    "checkout-master-branch": "git checkout master",
    "create-deploy-branch": "git checkout -b deploy",
    "add-build-directory": "sed -i '' 's|/build|#/build|' .gitignore",
    "commit-build-directory": "git add -A && git commit -m 'add build files'",
    "create-build": "npm run build && npm run add-build-directory && npm run commit-build-directory",
    "push-deploy": "git push origin `git subtree split --prefix=build`:gh-pages --force",
    "delete-deploy-branch": "npm run checkout-master-branch && git branch -D deploy",
    "deploy": "npm run checkout-master-branch && git pull && npm run create-deploy-branch && npm run create-build && npm run push-deploy && npm run delete-deploy-branch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
